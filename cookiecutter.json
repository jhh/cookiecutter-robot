{
  "frc_team": 2767,
  "project_name": "My Robot",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-') }}",
  "package_name": "frc.team{{ cookiecutter.frc_team }}.{{ cookiecutter.project_slug | replace('-', '') | replace('_', '') }}",
  "package_path": "{{ cookiecutter.package_name | replace('.', '/') }}",
  "use_kotlin": "n",
  "use_thirdcoast_swerve": "y",
  "use_thirdcoast_telemetry": "y",
  "use_driver_controls": "y",
  "use_game_controls": "y",
  "use_logger": "y",
  "use_spotless": "y",
  "_copy_without_render": [
    ".gitignore",
    "gradlew",
    "gradlew.bat",
    "gradle/wrapper/gradle-wrapper.jar",
    "vendordeps/phoenix.json",
    "vendordeps/navx.json"
  ]
}
